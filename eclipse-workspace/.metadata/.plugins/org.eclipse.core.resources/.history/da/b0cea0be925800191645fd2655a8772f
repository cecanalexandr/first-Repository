package com.class17;

public class calculator {

	public static void main(String[] args) {
		
		calculator demo=new calculator();
		demo.sum(10,20);
		demo.sub(100, 200);
		demo.mul(200, 300);
		demo.div(10, 20);
		

	}void sum(int a,int b){
		
		System.out.println(a+b);
	}
	void sub(int a,int b) {
		System.out.println(a-b);
	}
	void mul(int a,int b) {
		System.out.println(a*b);
	}void div(int a,int b){
		System.out.println(a/b);
	}

}
