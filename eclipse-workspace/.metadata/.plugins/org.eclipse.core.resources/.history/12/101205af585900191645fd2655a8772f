package pack1;

public class Application1 {
	public static void main(String[]args) {
		
		Animal obj=new Animal();
		
		System.out.println("This is obj");
		
		System.out.println(obj.age);
		System.out.println(obj.weight);
		System.out.println(obj.breed);
		System.out.println(obj.name);
		
		System.out.println(obj.canBark());
		System.out.println(obj.isWild());
		
		obj.name="Joe";
		obj.age=40;
		System.out.println(obj.age);
		
		Animal obj2=new Animal();
		
		System.out.println("This is obj2");
		
		
		System.out.println(obj2.age);
		System.out.println(obj2.weight);
		System.out.println(obj2.breed);
		System.out.println(obj2.name);
	
		Animal obj3=new Animal();
	System.out.println("This is obj3");
		System.out.println(obj3.age);
		System.out.println(obj3.weight);
		System.out.println(obj3.breed);
		System.out.println(obj3.name);
		
		
	}

}
