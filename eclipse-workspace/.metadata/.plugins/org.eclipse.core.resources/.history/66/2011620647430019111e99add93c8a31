package com.Class8;

public class forLoop {

	public static void main(String[] args) {
		
		//initiaze condition /testCondition increment /decrement
		
		for (int i=1; i <=5;i++) {
			
			System.out.println("Good morning "+i);
			
		}
		/*I want to print number from 10 to 1*/
		for(int i=10;i>=1;i--) {
			System.out.println(i);
		}
		
		
		
		
		
		
		
		
		
		

	}

}
