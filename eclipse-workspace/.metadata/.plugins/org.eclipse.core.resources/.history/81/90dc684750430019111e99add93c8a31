package com.Class8;

public class Sum {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int sum=0;
		
		for (int i=1;i<+5;i++) {
			sum=sum+i;
		}
		System.out.println(sum);//15
		
		/* find sum of all even number from 1 to 100*/
		sum=0;
		for (int a=1;a<=100; a=+2) {
			sum=sum+100; {
				System.out.println(a);
			}
		}
			
		
		
		
		
		
		
		
		
		
	}

}
