package pack1;

public class Calculator {
	int sum(int a,int b,int c) {
		return a+b+c;
	}
	int average(int a,int b,int c) {
		return a+b+c/3;
	}
	int max(int a,int b,int c) {
		if(a>b && a>c) {
			return a;
		}
		if(b>a && b>c) {
		return b;
	}
		else {
		return c;
	}
	
}
	int min(int a,int b,int c) {
		if(a>b && a>c) {
			return a;
		}
		if(b>a && b>c) {
		return b;
	}
		else {
		return c;
	}
	
}

}