package com.Class8;

public class Task {

	public static void main(String[] args) {
		/* Print numbers from1 to 100*/
		
  for(int a=1;  a<=100 ;a++) {
	  System.out.println(a);
  }
  for (int b=100;b>=1; b--) {
	  System.out.println(b);
  }
  for(int c=1;c<=20; c++) {
	  System.out.println(c);
  }
	
  for(int d=20;d>=1;d-- ) {
	  System.out.println(d);
  }
}
}
